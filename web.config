<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.webServer>
		 <rewrite>
			
		  </rewrite>
		<staticContent>
            <remove fileExtension=".woff" />
            <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
            <remove fileExtension=".woff2" />
            <mimeMap fileExtension=".woff2" mimeType="application/x-font-woff2" />
            <remove fileExtension=".ttf" />
            <mimeMap fileExtension=".ttf" mimeType="application/octet-stream" />
            <remove fileExtension=".mp4" />
            <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
            <remove fileExtension=".svg" />
        
	<mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
			<remove fileExtension=".m4v" />
            <mimeMap fileExtension=".m4v" mimeType="audio/mv4" />
			<remove fileExtension=".flac" />
            <mimeMap fileExtension=".flac" mimeType="audio/flac" />
			<remove fileExtension=".ogg" />
            <mimeMap fileExtension=".ogg" mimeType="audio/ogg" />
			<remove fileExtension=".obj" />
            <mimeMap fileExtension=".obj" mimeType="text/plain" />
	<remove fileExtension=".mtl" />
            <mimeMap fileExtension=".mtl" mimeType="text/plain" />
        </staticContent>
	</system.webServer>
	<system.web>
	  <customErrors mode="Off" />
	</system.web>
</configuration>
